<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Servlet_App_0</display-name>
  <welcome-file-list>
		<welcome-file>home.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>firstServlet</servlet-name>
		<servlet-class>com.nt.DateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>firstServlet</servlet-name>
		<url-pattern>/date</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>registerServlet</servlet-name>
		<servlet-class>com.nt.RegisterServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registerServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>updateServlet</servlet-name>
		<servlet-class>com.nt.UpdateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>updateServlet</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteServlet</servlet-name>
		<servlet-class>com.nt.DeleteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>deleteServlet</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>selectServlet</servlet-name>
		<servlet-class>com.nt.SelectServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>selectServlet</servlet-name>
		<url-pattern>/select</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>selectAllServlet</servlet-name>
		<servlet-class>com.nt.SelectAllServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>selectAllServlet</servlet-name>
		<url-pattern>/select-all</url-pattern>
	</servlet-mapping>
	
</web-app>